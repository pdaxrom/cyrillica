###### Combined headers


####### Compile

main.o: main.cpp \
		cyrkbdset.h \
		cyrkbdsetbase.h

cyrkbdset.o: cyrkbdset.cpp \
		cyrkbdset.h \
		cyrkbdsetbase.h

cyrkbdsetbase.h: cyrkbdsetbase.ui
	$(UIC) cyrkbdsetbase.ui -o $(INTERFACE_DECL_PATH)/cyrkbdsetbase.h

cyrkbdsetbase.cpp: cyrkbdsetbase.ui
	$(UIC) cyrkbdsetbase.ui -i cyrkbdsetbase.h -o cyrkbdsetbase.cpp

cyrkbdsetbase.o: cyrkbdsetbase.cpp \
		cyrkbdsetbase.h \
		cyrkbdsetbase.ui

moc_cyrkbdset.o: moc_cyrkbdset.cpp \
		cyrkbdset.h \
		cyrkbdsetbase.h

moc_cyrkbdsetbase.o: moc_cyrkbdsetbase.cpp \
		cyrkbdsetbase.h

moc_cyrkbdset.cpp: cyrkbdset.h
	$(MOC) cyrkbdset.h -o moc_cyrkbdset.cpp

moc_cyrkbdsetbase.cpp: cyrkbdsetbase.h
	$(MOC) cyrkbdsetbase.h -o moc_cyrkbdsetbase.cpp

