####### Compiler, tools and options

CC	=	arm-linux-gcc
CXX	=	arm-linux-g++
CFLAGS	=	-pipe -Wall -W -O2 -DNO_DEBUG
CXXFLAGS=	-pipe -DQT_QWS_EBX -DQT_QWS_CUSTOM -DQWS -fno-exceptions -fno-rtti -Wall -W -O2 -DNO_DEBUG
INCPATH	=	-I$(QTDIR)/include
LINK	=	arm-linux-gcc
LFLAGS	=	
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -lqpe -lqte
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

HEADERS =	cyrkbdset.h
SOURCES =	main.cpp \
		cyrkbdset.cpp
OBJECTS =	main.o \
		cyrkbdset.o \
		cyrkbdsetbase.o
INTERFACES =	cyrkbdsetbase.ui
UICDECLS =	cyrkbdsetbase.h
UICIMPLS =	cyrkbdsetbase.cpp
SRCMOC	=	moc_cyrkbdset.cpp \
		moc_cyrkbdsetbase.cpp
OBJMOC	=	moc_cyrkbdset.o \
		moc_cyrkbdsetbase.o
DIST	=	
TARGET	=	cyrkbdset
INTERFACE_DECL_PATH = .

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: cyrkbdset.pro
	tmake cyrkbdset.pro -o Makefile

dist:
	$(TAR) cyrkbdset.tar cyrkbdset.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) cyrkbdset.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

